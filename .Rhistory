gameOfCraps = function(p = 0){
x = sum(sample(1:6, 2, replace = T))
if (p == 0) {
if (x == 7 | x == 11){return("Win")}
else if (x %in% c(2, 3, 12)){return("Loss")}
else {
gameOfCraps(x)
}
}
else {
if (x == 7){return("Loss")}
else if (x == p){return("Win")}
else {gameOfCraps(p)}
}
}
